{
    "compilerOptions": {
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "module": "commonjs",
        "target": "esnext",
        "downlevelIteration": true,
        "skipLibCheck": true,
        "allowUmdGlobalAccess": true,
        "importHelpers": true
    },
    "exclude": ["node_modules"]
}
